<% content_for :head do -%>
  <%= javascript_pack_tag 'form_vue' %>
<% end -%>
<%= simple_form_for @course do |f| %>
  <%= f.input :name, label: "Course Title" %>
  <label>Professors</label>
  <br>
  <%= f.collection_check_boxes :professor_ids, Professor.all, :id, :name %>
  <br>
  <%= f.button :submit %>
<% end %>