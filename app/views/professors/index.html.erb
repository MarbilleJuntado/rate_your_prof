<%= javascript_pack_tag 'data_index' %>
<div id="index">
  <h1>Professors</h1>
  <h2 v-if="!professors">There are no existing professors</h2>
  <div v-else>
    <h2 v-for="professor of professors">
      <a :href="'/professors/' + professor.id">{{ professor.name }}</a>
    </h2>
  </div>
  <br>
  <%= link_to "List of Courses", courses_path %>
</div>
