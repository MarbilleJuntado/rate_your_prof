<% content_for :head do -%>
  <%= javascript_pack_tag 'form_vue' %>
<% end -%>
<%= simple_form_for @professor do |f| %>
  <%= f.input :name, label: "Professor Name" %>
  <label>Courses</label>
  <br>
  <%= f.collection_check_boxes :course_ids, Course.all, :id, :name %>
  <br>
  <%= f.button :submit %>
<% end %>